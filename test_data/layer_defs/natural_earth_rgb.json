{
  "id": "83866f7b-dcee-47b8-9242-e5636ceaf402",
  "name": "Natural Earth II â€“ RGB",
  "description": "A raster with three bands for RGB visualization",
  "workflow": {
    "type": "Raster",
    "operator": {
      "type": "RasterStacker",
      "params": {
        "renameBands": {
          "type": "rename",
          "values": ["blue", "green", "red"]
        }
      },
      "sources": {
        "rasters": [
          {
            "type": "GdalSource",
            "params": {
              "data": "ne2_raster_blue"
            }
          },
          {
            "type": "GdalSource",
            "params": {
              "data": "ne2_raster_green"
            }
          },
          {
            "type": "GdalSource",
            "params": {
              "data": "ne2_raster_red"
            }
          }
        ]
      }
    }
  },
  "symbology": {
    "type": "raster",
    "rasterColorizer": {
      "type": "multiBand",
      "redBand": 2,
      "greenBand": 1,
      "blueBand": 0,
      "redMin": 0,
      "redMax": 255,
      "redScale": 1,
      "greenMin": 0,
      "greenMax": 255,
      "greenScale": 1,
      "blueMin": 0,
      "blueMax": 255,
      "blueScale": 1,
      "noDataColor": [0, 0, 0, 0]
    },
    "opacity": 1.0
  }
}
